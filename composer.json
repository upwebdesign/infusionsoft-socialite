{
  "name": "upwebdesign/infusionsoft-socialite",
  "description": "Infusionsoft OAuth2 Provider for Laravel Socialite",
  "type": "plugin",
  "require": {
    "php": "^7.4|^8.0",
    "ext-json": "*",
    "illuminate/support": "^6.0|^7.0|^8.0|^9.0",
    "laravel/socialite": "^5.0|6.0"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "InfusionsoftSocialite\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "InfusionsoftSocialite\\InfusionsoftSocialiteServiceProvider"
      ]
    }
  },
  "authors": [
    {
      "name": "Jesse Vista",
      "email": "jesse@upwebdesign.com"
    }
  ],
  "minimum-stability": "dev"
}
